{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "google"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "camelcase": "off",
      "max-len": [
          "error",
          {
              "code": 150
          }
      ],
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single",
          {
              "avoidEscape": true
          }
      ],
      "semi": [
          "error",
          "never"
      ],
      "object-curly-spacing": [
          2,
          "always"
      ],
      "require-jsdoc": 0,
      "new-cap": "off"
  },
  "ignorePatterns": [
      "**/*.json"
  ]
}